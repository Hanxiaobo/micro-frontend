{
  "name": "bolt",
  "version": "0.1.0",
  "scripts": {
    "dev": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "rd": "npm run clear && vue-cli-service build --mode rd",
    "release": "npm run clear && vue-cli-service build --mode release",
    "clear": "rimraf dist",
    "prettier": "prettier \"{src,mock}/**/*.{vue,ts}\" --write",
    "lint:js": "eslint \"{src,mock}/**/*.{vue,ts}\" ",
    "lint:js-fix": "eslint \"{src,mock}/**/*.{vue,ts}\" --fix",
    "lint:css": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/",
    "lint:css-fix": "stylelint \"**/*.{vue,css,scss}\" --cache --cache-location node_modules/.cache/stylelint/ --fix",
    "test": "jest",
    "husky": "husky install"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "npm run prettier",
      "npm run lint:js-fix",
      "npm run lint:css-fix",
      "git add ."
    ]
  },
  "dependencies": {
    "@element-plus/icons-vue": "^1.1.4",
    "@vue/babel-plugin-jsx": "1.0.6",
    "axios": "0.21.1",
    "codemirror": "^5.65.2",
    "codemirror-editor-vue3": "^2.0.6",
    "core-js": "3.6.5",
    "cross-env": "^7.0.3",
    "js-cookie": "2.2.1",
    "nprogress": "0.2.0",
    "screenfull": "5.1.0",
    "sk-ui": "^0.1.19",
    "sql-formatter": "^2.3.3",
    "vue": "^3.2.37",
    "vue-clipboard3": "^2.0.0",
    "vue-fullscreen": "^3.1.1",
    "vue-router": "4.0.4",
    "vue-uuid": "^3.0.0",
    "vue3-ace-editor": "^2.2.2",
    "vuex": "4.0.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@arco-design/web-vue": "^2.21.2",
    "@babel/plugin-proposal-optional-chaining": "^7.16.7",
    "@commitlint/cli": "12.1.1",
    "@commitlint/config-conventional": "12.1.1",
    "@types/codemirror": "^5.60.5",
    "@types/jest": "26.0.22",
    "@types/js-cookie": "2.2.6",
    "@types/node": "14.14.31",
    "@types/nprogress": "0.2.0",
    "@typescript-eslint/eslint-plugin": "4.22.0",
    "@typescript-eslint/parser": "4.22.0",
    "@vitejs/plugin-vue": "1.1.4",
    "@vue/cli-plugin-babel": "4.5.0",
    "@vue/cli-plugin-eslint": "4.5.0",
    "@vue/cli-plugin-router": "4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.15",
    "@vue/cli-plugin-vuex": "4.5.0",
    "@vue/cli-service": "4.5.0",
    "@vue/compiler-sfc": "3.1.2",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "@vue/test-utils": "2.0.0-rc.6",
    "autoprefixer": "9.8.6",
    "babel-eslint": "10.1.0",
    "compression-webpack-plugin": "^5.0.1",
    "cz-conventional-changelog": "3.3.0",
    "eslint": "7.21.0",
    "eslint-plugin-vue": "7.7.0",
    "husky": "7.0.0",
    "jest": "26.6.3",
    "lint-staged": "10.5.4",
    "mockjs": "1.0.1-beta3",
    "prettier": "2.2.1",
    "sass": "1.32.8",
    "sass-loader": "8.0.2",
    "stylelint": "13.11.0",
    "stylelint-config-prettier": "8.0.2",
    "stylelint-config-standard": "20.0.0",
    "stylelint-order": "4.1.0",
    "ts-jest": "26.5.5",
    "typescript": "4.1.3",
    "vue-jest": "5.0.0-alpha.7",
    "webpack-bundle-analyzer": "4.5.0"
  },
  "license": "MIT",
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
